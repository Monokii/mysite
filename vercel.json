{
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 10 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/index.html",
      "destination": "/home"
    },
    {
      "source": "/site-pages/other/any.html",
      "destination": "/any"
    },
    {
      "source": "/site-pages/docs/docs.html",
      "destination": "/docs"
    }
  ]
}